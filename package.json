{
  "name": "mf-tirillas-despachos",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn && webpack serve --mode production",
    "dev": "yarn && webpack serve --mode development --host 0.0.0.0",
    "start:test": "yarn && webpack serve --mode development --env test",
    "build": "yarn && webpack --mode production",
    "build:dev": "yarn && webpack --mode development",
    "build:test": "yarn && webpack --mode development --env test",
    "lint": "eslint src/*",
    "deploy": "yarn build && gcloud app deploy --project cm-tim-goo --quiet && rm -rf public/",
    "deploy:dev": "yarn build:dev && gcloud app deploy --project cm-tim-goo-dev --quiet && rm -rf public/",
    "deploy:test": "yarn build:test && gcloud app deploy --project cm-tim-goo-test --quiet && rm -rf public/"
  },
  "repository": "git@github.com:PipeOspina/mf-tirillas-despachos.git",
  "author": "Felipe Ospina <pipe.98.oh@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-beta.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^5.0.0-beta.2",
    "@progress/kendo-drawing": "^1.11.0",
    "@progress/kendo-licensing": "^1.2.0",
    "@progress/kendo-react-pdf": "^4.8.0",
    "firebase": "^8.8.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "css-loader": "^6.2.0",
    "dotenv-webpack": "^7.0.3",
    "esbuild-loader": "^2.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.0",
    "style-loader": "^3.2.1",
    "typescript": "^4.3.5",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
